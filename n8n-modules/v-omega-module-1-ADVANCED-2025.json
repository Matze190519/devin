        "jsCode": "// Advanced Content Intelligence Fusion Engine 2025\n// Combines all harvested data into viral-optimized content strategy\n\nconst perplexityData = $input.first()?.json || {};\nconst claudeContent = $input.all().find(item => item.json?.content)?.json || {};\nconst viralScores = $input.all().find(item => item.json?.viral_score)?.json || {};\nconst redditTrends = $input.all().find(item => item.json?.data?.children)?.json?.data?.children || [];\nconst youtubeTrends = $input.all().find(item => item.json?.items)?.json?.items || [];\nconst scrapeCreatorsTrending = $input.all().find(item => item.json?.trending_posts)?.json?.trending_posts || []; [];\nconst instagramData = $input.all().find(item => item.json?.data?.posts)?.json?.data?.posts || [];\nconst newsData = $input.all().find(item => item.json?.articles)?.json?.articles || [];\n\n// Advanced Trend Analysis\nconst trendingKeywords = [];\nconst viralPatterns = [];\nconst contentOpportunities = [];\n\n// Extract trending keywords from all sources\nredditTrends.forEach(post => {\n  if (post.data?.title) {\n    const keywords = post.data.title.toLowerCase().match(/\\b\\w{4,}\\b/g) || [];\n    trendingKeywords.push(...keywords);\n  }\n});\n\nyoutubeTrends.forEach(video => {\n  if (video.snippet?.title) {\n    const keywords = video.snippet.title.toLowerCase().match(/\\b\\w{4,}\\b/g) || [];\n    trendingKeywords.push(...keywords);\n  }\n});\n\n// Keyword frequency analysis\nconst keywordFreq = {};\ntrendingKeywords.forEach(keyword => {\n  keywordFreq[keyword] = (keywordFreq[keyword] || 0) + 1;\n});\n\nconst topKeywords = Object.entries(keywordFreq)\n  .sort(([,a], [,b]) => b - a)\n  .slice(0, 20)\n  .map(([keyword]) => keyword);\n\n// Generate viral content recommendations\nconst viralContentStrategy = {\n  timestamp: new Date().toISOString(),\n  trend_analysis: {\n    top_keywords: topKeywords,\n    trending_topics: [\n      \"passive income automation\",\n      \"network marketing success stories\",\n  