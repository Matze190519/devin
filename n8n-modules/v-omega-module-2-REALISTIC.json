    {
      "parameters": {
        "jsCode": "// Lead Scoring Algorithm (Realistic)\nconst leadData = $input.first().json;\nlet leadScore = 0;\n\n// Income goal scoring (30% weight)\nif (leadData.desired_income >= 5000) leadScore += 30;\nelse if (leadData.desired_income >= 3000) leadScore += 25;\nelse if (leadData.desired_income >= 1500) leadScore += 20;\nelse leadScore += 10;\n\n// Experience level scoring (25% weight)\nif (leadData.network_marketing_experience === 'Profi') leadScore += 25;\nelse if (leadData.network_marketing_experience === 'Erfahren') leadScore += 20;\nelse if (leadData.network_marketing_experience === 'Wenig') leadScore += 15;\nelse leadScore += 10; // Keine Erfahrung\n\n// Car category interest (20% weight)\nif (['Sportwagen', 'Luxuslimousine'].includes(leadData.car_category)) leadScore += 20;\nelse if (['SUV', 'Elektroauto'].includes(leadData.car_category)) leadScore += 15;\nelse leadScore += 10;\n\n// Engagement indicators (15% weight)\nif (leadData.phone) leadScore += 8;\nif (leadData.company) leadScore += 7;\n\n// Data completeness (10% weight)\nconst completedFields = Object.values(leadData).filter(v => v && v !== '').length;\nleadScore += Math.min(10, completedFields);\n\n// Lead quality classification\nlet leadQuality = 'cold';\nif (leadScore >= 80) leadQuality = 'hot';\nelse if (leadScore >= 60) leadQuality = 'warm';\nelse if (leadScore >= 40) leadQuality = 'lukewarm';\n\nreturn {\n  ...leadData,\n  lead_score: leadScore,\n  lead_quality: leadQuality,\n  follow_up_priority: leadScore >= 70 ? 'high' : leadScore >= 50 ? 'medium' : 'low',\n  recommended_next_action: leadScore >= 70 ? 'immediate_call' : leadScore >= 50 ? 'email_sequence' : 'nurture_campaign'\n};"
      },
