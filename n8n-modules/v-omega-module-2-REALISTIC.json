      "parameters": {
        "jsCode": "// Personality Analysis & Avatar Matching\nconst leadData = $input.first().json;\nconst enrichedData = $input.last().json;\n\n// Analyze personality based on form responses and enriched data\nlet personalityProfile = {\n  risk_tolerance: 'medium',\n  communication_style: 'professional',\n  motivation_type: 'achievement',\n  avatar_match: 'success_mentor'\n};\n\n// Risk tolerance analysis\nif (leadData.desired_income > 5000) {\n  personalityProfile.risk_tolerance = 'high';\n  personalityProfile.avatar_match = 'luxury_lifestyle';\n} else if (leadData.desired_income < 2000) {\n  personalityProfile.risk_tolerance = 'low';\n  personalityProfile.avatar_match = 'gentle_guide';\n}\n\n// Communication style based on car preference\nif (leadData.car_category === 'Sportwagen') {\n  personalityProfile.communication_style = 'dynamic';\n  personalityProfile.motivation_type = 'excitement';\n} else if (leadData.car_category === 'Luxuslimousine') {\n  personalityProfile.communication_style = 'sophisticated';\n  personalityProfile.motivation_type = 'status';\n}\n\n// Experience level adjustment\nif (leadData.network_marketing_experience === 'Keine') {\n  personalityProfile.avatar_match = 'beginner_friendly';\n  personalityProfile.communication_style = 'educational';\n} else if (leadData.network_marketing_experience === 'Profi') {\n  personalityProfile.avatar_match = 'peer_expert';\n  personalityProfile.communication_style = 'strategic';\n}\n\nreturn {\n  ...leadData,\n  ...enrichedData,\n  personality_profile: personalityProfile,\n  avatar_script_tone: personalityProfile.communication_style,\n  recommended_approach: personalityProfile.motivation_type\n};"
      },
