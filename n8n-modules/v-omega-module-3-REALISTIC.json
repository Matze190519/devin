      "parameters": {
        "jsCode": "// Product Filtering & Selection Logic\nconst products = $('google-sheets-product-selector').first().json;\nconst leadData = $input.first().json;\n\n// Filter products based on lead profile\nconst filteredProducts = products.filter(product => {\n  // Price filter based on desired income\n  const price = parseFloat(product.price || 0);\n  const maxPrice = leadData.desired_income * 0.1; // 10% of desired monthly income\n  \n  if (price > maxPrice) return false;\n  \n  // Category matching\n  if (leadData.car_category === 'Sportwagen' && product.category === 'Energy') return true;\n  if (leadData.car_category === 'Luxuslimousine' && product.category === 'Beauty') return true;\n  if (leadData.car_category === 'SUV' && product.category === 'Health') return true;\n  if (leadData.car_category === 'Elektroauto' && product.category === 'Wellness') return true;\n  \n  // Default to starter products for beginners\n  if (leadData.network_marketing_experience === 'Keine' && product.starter_friendly) return true;\n  \n  return false;\n});\n\n// Select top 3 products\nconst selectedProducts = filteredProducts\n  .sort((a, b) => parseFloat(b.commission_rate) - parseFloat(a.commission_rate))\n  .slice(0, 3);\n\nreturn {\n  ...leadData,\n  recommended_products: selectedProducts,\n  product_count: selectedProducts.length,\n  total_commission_potential: selectedProducts.reduce((sum, p) => sum + parseFloat(p.commission_rate || 0), 0)\n};"
      },
      "id": "product-filtering-logic",
      "name": "üéØ Product Filtering Logic",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
